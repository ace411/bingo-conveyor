{
    "name": "chemem/bingo-conveyor",
    "description": "A simple HTML view conveyor for the Bingo Framework.",
    "type": "library",
    "authors": [
        {
            "name": "Lochemem Bruno Michael",
            "email": "lochbm@gmail.com"
        }
    ],
    "require-dev": {
